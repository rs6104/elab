using namespace std;
int main()
{
 int p,q,r,i;
 int c;
 cin>>c;
 for(i=0;i<c;i++){
 cin>>p>>q>>r;
 q=q+(r-1)/5;
 r=(r-1)%5+1;
 p=p+(q-1)/10;
 q=(q-1)%10+1;
 cout<<p<<" ";
 cout<<q<<" ";
 cout<<r<<endl;
 }
return 0;
}
#######################################Tina owns a match making company, which even to her :-
#include<bits/stdc++.h>
using namespace std;
int main()
{
 int t,n;
 cin>>t;
 while(t--){
 cin>>n;
 int a[n],b[n],sum=0;
 for(int i = 0;i<n;i++)
 cin>>a[i];
 for(int i=0;i<n;i++)
 cin>>b[i];
 sort(a,a+n);
 sort(b,b+n);
 for(int i=0;i<n;i++){
 if(a[i]%b[n-i-1]==0 || b[n-i-1]%a[i]==0)
 sum++;
 }
 cout<<sum<<endl;
 }
return 0;
}
