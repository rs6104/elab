	m-=n-1;
	if(m)for(int i=1;i<=n;++i)for(int j=i+1;j<=n;++j)if(i!=v && j!=u && i!=u && j!=v){
		printf("%d %d\n",i,j);
		m--;
		if(!m)return 0;
	}
}


